{
  "$id": "answersIndexingApi",
  "$schema": "https://schema.yext.com/config/streams/streams-endpoint/v2",
  "name": "Answers Indexing Endpoint",
  "stream": {
    "source": "content",
    "fields": [
      "address",
      "id",
      "mainPhone",
      "meta"
    ],
    "filter": {
      "entityTypes": [
        "healthcareProfessional"
      ]
    },
    "localization": {
      "locales": [
        "en"
      ],
      "excludePrimary": true
    }
  },
  "fieldIndexes": [
    {
      "field": "id"
    }
  ]
}
